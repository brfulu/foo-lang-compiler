import 'io';

int num = io.in();
int sum = 0;

loop
  .init()
  .cond(num > 0)
  .step(num = num / 10)
  .body(
    int digit = num % 10;
    sum = sum + digit;
  )

if
  .cond(sum < 10)
  .then(
    io.out('Suma cifara je manja od 10');
  )
  .else(
    if
      .cond(sum == 10)
      .then(
        io.out('Suma cifara je 10');
      )
      .else(
        io.out('Suma cifara je veca od 10');
      )
  )