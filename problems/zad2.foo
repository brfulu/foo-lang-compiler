import 'io';

int num = io.in.call();
int sum = 0;

loop
  .init()
  .cond(num > 0)
  .body(
    int digit = num % 10;
    sum += digit;
    num /= 10;
  )

str result = '';

if
  .cond(sum < 10)
  .then(
    result = 'Suma cifara je manja od 10';
  )
  .else(
    if
      .cond(sum == 10)
      .then(
        result = 'Suma cifara je 10';
      )
      .else(
        result = 'Suma cifara je veca od 10';
      )
  )

io.out.call(result);