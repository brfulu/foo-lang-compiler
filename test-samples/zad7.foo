import 'io';
import 'string';

str text = io.in.call();
str result = '';

loop
  .init(
    int i = 0;
  )
  .cond(i < text.len.call() - 1)
  .body(
    result += text[i];

    if
      .cond(string.isalpha.call(text[i]) and string.isdigit.call(text[i+1]))
      .then(
        result += '#';
      )
      
    if
      .cond(string.isdigit.call(text[i]) and string.isalpha.call(text[i+1]))
      .then(
        result += '*';
      )

    ++i;
  );

io.out.call(result);