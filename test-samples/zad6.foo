import 'io';
import 'math';

list result;
int x = io.in.call();

loop
  .init()
  .cond(x >= 0)
  .body(
    int temp = math.sqrt.call(x);
    if
      .cond(temp * temp == x)
      .then(
        result.append.call(x);
      );
    x = io.in.call();
  );

io.out.call('Perfect squares');
loop
  .init(
    int i = 0;
  )
  .cond(i < result.len.call())
  .body(
    io.out.call(result[i]);
    ++i;
  );